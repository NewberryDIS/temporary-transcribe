generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model Item {
  id              Int      @id
  pagecount       Int?
  title           String?
  description     String?
  image           String?
  catalogLink     String?
  percentComplete Int?
  public          Boolean?
  heatured        Boolean?
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model Page {
  id            Int     @id
  itemid        Int?
  omekafn       String?
  resx          Int?
  resy          Int?
  transcription String? @default("")
}
