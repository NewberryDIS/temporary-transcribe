#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/svelte-check@3.3.1_postcss@8.4.23_svelte@3.59.0/node_modules/svelte-check/bin/node_modules:/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/svelte-check@3.3.1_postcss@8.4.23_svelte@3.59.0/node_modules/svelte-check/node_modules:/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/svelte-check@3.3.1_postcss@8.4.23_svelte@3.59.0/node_modules:/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/svelte-check@3.3.1_postcss@8.4.23_svelte@3.59.0/node_modules/svelte-check/bin/node_modules:/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/svelte-check@3.3.1_postcss@8.4.23_svelte@3.59.0/node_modules/svelte-check/node_modules:/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/svelte-check@3.3.1_postcss@8.4.23_svelte@3.59.0/node_modules:/Users/nickwhite/Documents/gitlab_newberry/temporary-transcribe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
